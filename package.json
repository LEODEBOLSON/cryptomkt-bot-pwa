    "@vue/eslint-config-prettier": "^4.0.1",
    "eslint": "^5.15.3",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "node-sass": "^4.9.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
